{
  "goal": "Login to https://www.saucedemo.com and return the Bike Light name and price",
  "steps": [
    {
      "tool": "navigate",
      "args": {
        "url": "https://www.saucedemo.com"
      }
    },
    {
      "tool": "type",
      "args": {
        "selector": "[data-test='username']",
        "text": "standard_user",
        "clear": true
      }
    },
    {
      "tool": "type",
      "args": {
        "selector": "[data-test='password']",
        "text": "secret_sauce",
        "clear": true
      }
    },
    {
      "tool": "click",
      "args": {
        "selector": "[data-test='login-button']"
      }
    },
    {
      "tool": "wait_for",
      "args": {
        "selector": ".inventory_list",
        "state": "visible"
      }
    },
    {
      "tool": "extract_text",
      "args": {
        "selector": ".inventory_item:has-text(\"Bike Light\") .inventory_item_name",
        "id": "bike_light_name"
      }
    },
    {
      "tool": "extract_text",
      "args": {
        "selector": ".inventory_item:has-text(\"Bike Light\") .inventory_item_price",
        "id": "bike_light_price"
      }
    }
  ],
  "final_report": "{\"goal\":\"Login to https://www.saucedemo.com and return the Bike Light name and price\",\"extracted\":{\"bike_light_name\":\"{bike_light_name}\",\"bike_light_price\":\"{bike_light_price}\"}}"
}